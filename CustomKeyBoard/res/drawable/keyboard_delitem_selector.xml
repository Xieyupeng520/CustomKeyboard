<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 触摸模式下(无焦点，有点击的时候)按下的时候 -->
    <item android:state_pressed="true" android:state_focused="false" android:drawable="@drawable/icon_back_press"></item>
    <!-- 非触摸模式下(有焦点，有点击的时候)按下的时候 -->
    <item android:state_pressed="true" android:state_focused="true" android:drawable="@drawable/icon_back_press" ></item>
    <!-- 无焦点的时候 -->
    <item android:state_focused="false" android:drawable="@drawable/icon_back"></item>
    <!-- 有焦点的时候 -->
    <item android:state_focused="true" android:drawable="@drawable/icon_back_focus"></item>
</selector>
